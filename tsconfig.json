{
    "compilerOptions": {
        "allowJs": true,
        "target": "es6",
        "baseUrl": "./",
        "module": "CommonJS",
        "lib": ["es6", "dom"],
        "types": ["cypress", "node", "cypress-file-upload", "cypress-real-events", "cypress-plugin-api"],
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmit": true
    },
    "include": ["**/*.ts", "**/*.json", "cypress.config.ts", "./cypress/scripts/*.ts"]
}
